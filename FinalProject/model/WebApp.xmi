<?xml version="1.0" encoding="UTF-8"?>
<WA:WebApp xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:WA="http://www.unex.es/dmss/webapp"
    xsi:schemaLocation="http://www.unex.es/dmss/webapp ../metamodel/WebPage.ecore"
    name="DMSS">
  <questions
      text="Question1"
      type="SHORT"/>
  <questions
      text="Question2"
      visualRep="LINEAL_CHART"/>
  <questions
      text="Question3"
      type="BOOLEAN"
      visualRep="PIE_CHART"/>
  <questions
      text="Question20"
      type="LONG"
      visualRep="TEXTUAL"/>
  <entities
      name="Students"
      crud="//@webpages.2">
    <references
        referendecEntity="//@entities.1"/>
    <attributes
        name="nif"
        type="String"/>
    <attributes
        name="name"
        type="String"/>
  </entities>
  <entities
      name="Subjects">
    <references
        referendecEntity="//@entities.0"/>
    <attributes
        name="name"
        type="String"/>
    <attributes
        name="Code"
        type="String"/>
  </entities>
  <webpages
      xsi:type="WA:Home"
      name="Index"/>
  <webpages
      xsi:type="WA:Index"
      name="Subjects"
      socialnetwork="//@socialNetworks.0"/>
  <webpages
      xsi:type="WA:CRUD"
      name="Students">
    <links xsi:type="WA:Internal"
        name="go to subject"
        target="//@webpages.1"/>
    <links xsi:type="WA:Internal"
        name="Go to home"
        target="//@webpages.0"/>
  </webpages>
  <webpages
      xsi:type="WA:Questionnary"
      name="Questionnary for students"
      question="//@questions.1 //@questions.3"
      feedback="true"/>
  <webpages
      xsi:type="WA:Survey"
      name="Survey for teachers"
      question="//@questions.0 //@questions.2"/>
  <socialNetworks
      name="TwitterTimeLine"/>
</WA:WebApp>
